{
  "$schema": "http://json-schema.org/schema",
  "id": "VicodersCliExampleHandler",
  "title": "Vicoders CLI example handler",
  "type": "object",
  "description": "Example handler",
  "properties": {
    "plugin": {
      "description": "Which plugin do you want to install?",
